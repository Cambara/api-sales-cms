{
    "version": 2,
    "buildCommand": "npm run db:migrate",
    "outputDirectory": "dist",
    "functions": {
        "src/main.js": {
          "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/main.js"
        }
    ]
}