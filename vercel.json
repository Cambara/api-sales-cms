{
    "version": 2,
    "buildCommand": "npm run db:migrate",
    "outputDirectory": "dist",
    "functions": {
        "src/main.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/main.js"
        }
    ]
}