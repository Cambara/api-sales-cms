{
    "version": 2,
    "buildCommand": "npm run db:migrate",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}