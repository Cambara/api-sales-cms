{
    "version": 2,
    "buildCommand": "npm run db:migrate",
    "outputDirectory": "dist",
    "functions": {
        "src/main.js": {
            "memory": 3008,
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/main.js"
        }
    ]
}